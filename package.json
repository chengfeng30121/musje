{
  "name": "musje",
  "version": "0.4.1",
  "description": "123 jianpu music processor for QinJianPu",
  "main": "lib/musje.js",
  "scripts": {
    "test": "mocha src/**/__tests__/*.js --require ./src/__tests__/helper --inline-diffs",
    "clean": "rimraf lib doc",
    "build": "npm run build:css && npm run build:dev && npm run build:min && npm run build:codemirror && npm run build:doc",
    "build:dev": "webpack --config webpack.config.js",
    "build:min": "webpack --config webpack.min.config.js",
    "build:codemirror": "webpack --config src/addon/codemirror/webpack.config.js",
    "build:doc": "jsdoc -c jsdoc.config.json",
    "build:css": "copyfiles -u 1 \"src/**/*.css\" lib && copyfiles -u 1 \"src/*.css\" lib",
    "watch": "webpack --config webpack.config.js --watch",
    "serve": "browser-sync start --server --files \"lib/**/*,demo/**/*\" --startPath \"/demo/\"",
    "dev": "concurrently \"npm run watch\" \"npm run serve\"",
    "demo": "npm run build && browser-sync start --server --files \"lib/**/*,demo/**/*\" --startPath \"/demo/\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chengfeng30121/musje.git"
  },
  "author": "Malcom Wu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chengfeng30121/musje/issues"
  },
  "homepage": "https://github.com/chengfeng30121/musje",
  "dependencies": {
    "jison": "^0.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "babel-loader": "^9.1.3",
    "browser-sync": "^3.0.2",
    "chai": "^5.1.1",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "jsdoc": "^4.0.3",
    "mocha": "^10.7.3",
    "rimraf": "^6.0.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}